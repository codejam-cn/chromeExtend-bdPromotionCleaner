{
    "manifest_version": 2,
    "name": "百度推广清除",
    "version": "0.1",
    "description": "百度推广清除，把百度搜索结果中的百度推广内容清理掉",
    "icons": {
        "16": "images/baidu16.png",
        "48": "images/baidu48.png",
        "128": "images/baidu128.png"
    },
    "permissions": [       
		    "activeTab"			
    ],	
	//  "background": {
	// 		"scripts": [ "background.js" ]
	// },
	"content_scripts": [
     {
       "matches": ["https://www.baidu.com/*","http://www.baidu.com/*"],       
       "js": ["zepto.min.js","front.js"]
     }
   ]
}
